<%- include('../partials/header.ejs')%>
<main class="swap-item-main">
    <img src="<%= item.img%>" alt="item image">
    <div class="item-info">
        <h2><%=item.name%> | <p><%= item.category%></p>
        </h2>
        <p><%=item.seller%></p>
        <p><%=item.description%></p>
        <form><button type="submit" formmethod="GET" formaction="/swaps/<%= item.id%>/edit">Edit</button>
            <button type="submit" formmethod="POST" formaction="/swaps/<%= item.id%>?_method=DELETE">Delete</button>
        </form>
    </div>
</main>
<%- include('../partials/footer.ejs')%>